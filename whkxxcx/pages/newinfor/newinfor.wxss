/* pages/newinfor/newinfor.wxss */

.item {
  height:160rpx;
  margin:20rpx 10rpx;
  overflow:hidden;
  border-bottom:1px solid #ddd;
  padding:10px 0;
}
.item .img {
  width:200rpx;
  margin-right:20rpx;
  float:left;
  height:160rpx;

}
.titlemsg {
  display: block;
  margin: 0 auto;
  line-height:1.4;
  font-size:30rpx;
  overflow:hidden;
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
  padding-right: 6px;
}
.description {
  font-size: 13px;
  color:#666666;
  overflow:hidden;
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
  margin-top:6px;
}
.listover{
   display: none;
}
.goTop{
  height:76rpx;
  width:76rpx;
  position:fixed;
  bottom:30px;
  right:30rpx;
}

.seeNum{
  background: url("data:image/png;base64,");
  background-size: 32rpx 32rpx;
  -moz-background-size:32rpx 32rpx;
  padding-left: 8%;
  background-repeat:no-repeat;
  background-position-y:center;
}

.weui-media-box__info {
   color: #666;
   font-size: 26rpx;
   line-height: 1.5em;
}

