<view class="page">
   
    <view class="page__bd">
        <view class="weui-form-preview">
            <view class="weui-form-preview__hd">
                <view class="weui-form-preview__item">
                    <view class="weui-form-preview__label">{{ cname }}</view>
                </view>
            </view>
            <view class="weui-form-preview__bd">
                <view class="weui-form-preview__item">
                    <view class="weui-form-preview__label">姓名</view>
                    <view class="weui-form-preview__value">{{ name }}</view>
                </view>
                <view class="weui-form-preview__item">
                    <view class="weui-form-preview__label">手机号</view>
                    <view class="weui-form-preview__value">{{ phone }}</view>
                </view>
                <view class="weui-form-preview__item">
                    <view class="weui-form-preview__label">身份证号</view>
                    <view class="weui-form-preview__value">{{ card }}</view>
                </view>
                <view class="weui-form-preview__item">
                    <view class="weui-form-preview__label">接车地点</view>
                    <view class="weui-form-preview__value">{{ place }}</view>
                </view>
                <view class="weui-form-preview__item">
                    <view class="weui-form-preview__label">活动时间</view>
                    <view class="weui-form-preview__value">{{ date }}</view>
                </view>
            </view>
            <view class="weui-form-preview__ft">
                 <view  class="weui-form-preview__btn weui-form-preview__btn_primary stateColor" wx-if="{{state=='报名成功'}}">{{ state }}</view>
                 <view  class="weui-form-preview__btn weui-form-preview__btn_primary stateColor2" wx-if="{{state=='审核中'}}">{{ state }}</view>
                 <view  class="weui-form-preview__btn weui-form-preview__btn_primary stateColor3" wx-if="{{state=='报名失败'}}">{{ state }}</view>
            </view>
        </view>
    </view>
</view>